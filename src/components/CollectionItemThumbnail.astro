---
const { sketch } = Astro.props;
---
<div
    class="collectionLinkContainer"
    data-script-src={`/src/scripts/sketches/${sketch.id}.ts`}
    data-sketch-id={`${sketch.id}`}
>
    <a href={`/${sketch.id}`} class="collectionLink">
        <div id={`thumbnail-animation-${sketch.id}`} class="collectionThumbAnimation"></div>
    </a>
</div>
